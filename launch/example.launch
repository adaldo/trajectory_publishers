<launch>

  <arg name='type'/>
  <rosparam file='$(find trajpub)/params/$(arg type).yaml'/>
  <rosparam file='$(find trajpub)/params/plotter.yaml'/>

  <node
    name='$(arg type)'
    pkg='trajpub'
    type='$(arg type).py'/>

  <node
    name='plotter'
    pkg='trajpub'
    type='plotter.py'/>

</launch>
